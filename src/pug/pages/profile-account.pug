block content
    

    +b.SECTION.protocol-info
        .container
            +b.H3.title(class="--after --center") The liquidity protocol

            +e.P.price <span class="color-primary">$</span> 49,950,800.00
            
            +e.description
                p Coindeks.com is an innovative cryptocurrency platform that includes several sought-after investment products, such as:
            
            .swiper-container.js-swiper-protocol.swiper-no-swiping
                .swiper-wrapper
                    .swiper-slide 
                        +b.counter-item 
                            +e.P.value $ 15,789
                            +e.P.description Account balance
                
                    .swiper-slide 
                        +b.counter-item 
                            +e.P.value $ 15,000
                            +e.P.description Blocked
                
                    .swiper-slide 
                        +b.counter-item 
                            +e.P.value $ 729
                            +e.P.description Withdraw availiable

    +b.SECTION.token-info
        .container  
            +e.row
                +e.text
                    +e.description
                        p <strong class="color-primary">CRY token</strong> 2 rounds token sale
                    +e.P.value $ 12,859,871,431.78
                +e.right-content
                    +b.buy-now
                        +b.A(href="#" class="btn --primary") Buy now
                        +e.arrow
                            +icon('001-right-arrow')
                        +e.UL.list 
                            +e.LI
                                a(href="#" target="_blank") 
                                    img(src="assets/images/buy-now/1.png", alt="")
                            +e.LI
                                a(href="#" target="_blank") 
                                    img(src="assets/images/buy-now/2.png", alt="")
                            +e.LI
                                a(href="#" target="_blank") 
                                    img(src="assets/images/buy-now/3.png", alt="")

