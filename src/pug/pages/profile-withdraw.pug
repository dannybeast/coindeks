                                            +e.icon 
                                                img(src="assets/images/currency/usdt.svg", alt="")
                                            +e.P.name USDT
                                        +e.right 
                                            +e.P.value 0.0001
                                            +e.bottom
                                                p Min: 0.000500
                                                p 0.00 USD
                            li
                                +b.LABEL.currency-label 
                                    input(type="radio" name="currency" value="dai")
