        +b.SECTION
            .container
                +b.block-yellow

                    +b.field(class="--horizontal --subtext")
                        label Enter amount:
                        input(type="number" name="amount" value="0.125")
                        +e.P.subtext BTC

                    +b.field(class="--horizontal")
                        label Send to:
                        input(type="text" name="address")

                        +b.BUTTON( class="btn --black")
                            span Withdraw funds
                            +e.SPAN.icon-wrap
                                +icon('001-right-arrow')
    +b.SECTION
        mixin tr(index)
            tr
                td 00#{index}
                td Bitcoin
                td
                    strong 0.000123 BTC
                td 12:33:22   01.01.2022
                td
                    +b.status(class="--success") Compleated
            tr
                td 00#{index}
                td Bitcoin
                td
                    strong 0.000123 BTC
                td 12:33:22   01.01.2022
                td
                    +b.status(class="--danger") Delayed
            tr
                td 00#{index}
                td Bitcoin
                td
                    strong 0.000123 BTC
                td 12:33:22   01.01.2022
                td
                    +b.status Canceled


        .container
            +b.H3.title(class="--small") Your Withdrawals List

            +b.block.table-filter-hidden
                table.js-datatables.responsive.nowrap(
                    data-page-length="10" )
                    thead
                        tr
                            th Num
                            th Currency
                            th Amount
                            th Date
                            th Status
                    tbody
                        each val in [1, 2, 3, 4, 5,6,7,8,9]
                            +tr(val)



